<div class="main">
  <div class="hero">
    {% section 'aim' %}
    {% section 'hero' %}
  </div>
  <div class="section-title big"><div class="section">4</div>OUR LAST PRODUCTS</div>
  <div class="view"><a href="collections/all">VIEW ALL</a></div>
  {% assign product_limit = 4 %}
  {% for product in collections.all.products limit:product_limit %}
      {% include 'product-body' %}
  {% endfor %}
  
 
  {% section 'about' %}
  <div class="collections">
    {% assign filtered_collections = collections | where_exp: "collection", "collection.title != 'SPECIAL' and collection.title != 'ALL'" %}
    {% assign sorted_collections = filtered_collections | sort: 'title' %}
    
    {% assign count = 0 %}
    
    {% for collection in sorted_collections %}
      {% if count < 3 %}
        {% unless collection.title == 'ALL' or collection.title == 'SPECIAL' %}
          <div class="collection">
            <div class="section-title big" style="margin-bottom:2px">
              <div class="section">{{ collection.all_products_count }}</div>{{ collection.title }}
            </div>
            <a href="{{ collection.url }}">
              <img src="{{ collection.image | collection_img_url: 'master' }}" alt="{{ collection.title }}">
            </a>
          </div>
          {% assign count = count | plus: 1 %}
        {% endunless %}
      {% endif %}
    {% endfor %}
    
    
  </div>
</div>
